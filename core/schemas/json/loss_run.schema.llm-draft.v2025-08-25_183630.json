{
  "type": "object",
  "properties": {
    "gross_paid": {
      "type": "number",
      "description": "Total amount paid for the claim.",
      "examples": [
        180000.0,
        170000.0,
        90000.0
      ],
      "minimum": 0
    },
    "cause": {
      "type": "string",
      "description": "The reason for the loss.",
      "examples": [
        "Water damage (sprinkler leak)",
        "Theft",
        "Storm â€“ roof damage"
      ]
    },
    "status": {
      "type": "string",
      "description": "Current status of the claim.",
